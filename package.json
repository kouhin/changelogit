{
  "name": "changelogit",
  "version": "0.0.4",
  "description": "A JavaScript-Library version of [git-extras/git-changelog](https://github.com/tj/git-extras/blob/master/bin/git-changelog)",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run test && npm run build",
    "test": "eslint src",
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib coverage .nyc_output",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/changelogit.git"
  },
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "changelog",
    "git",
    "history"
  ],
  "author": "Bin Hou <houbin217jz@gmail.com> (https://twitter.com/houbin217jz)",
  "license": "MIT",
  "dependencies": {
    "@types/github": "^7.1.0",
    "github": "^14.0.0",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "microbundle": "^0.12.0",
    "rimraf": "^3.0.2"
  }
}
